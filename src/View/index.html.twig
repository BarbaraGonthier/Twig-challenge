{% extends "base.html.twig" %}

{% block title %} Index {% endblock %}

{% block content %}
  <div class="container">
    <div class="row">
    {% for product in products %}
        <div class="card" style="width: 18rem;">
            <div class="card-body">
               <h2 class="card-title">{{ product }}</h2>
           </div>
        </div>
    {%  endfor %}
  </div>
{% endblock %}
